<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="field">

    <class name="ch.globaz.tmmas.rentesservice.domain.model.Rente" table="rente" mutable="true">
        <id name="id" column="id">
            <generator class="org.hibernate.id.IdentityGenerator"/>
        </id>

        <property name="numero" column="numero" not-null="true"/>

        <property name="requerantId" column="requerantId" not-null="true"/>

        <property name="dateEnregistrement" column="dateEnregistrement" not-null="true"/>



    </class>

</hibernate-mapping>